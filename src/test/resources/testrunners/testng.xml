<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="3" thread-count ="3" parallel = "tests">


	<test name="Google Test">
		<parameter name="url" value="https://www.google.com" />
		<parameter name="browser" value="chrome" />


		<classes>
			<class name="testngsesstions.GoogleTest"/>
		</classes>
	</test>


	<test name="OrangeHRM Test">
		<parameter name="url" value="https://www.orangehrm.com/en/book-a-free-demo/" />
		<parameter name="browser" value="firefox" />


		<classes>
			<class name="testngsesstions.OrangeHRMTest"/>
		</classes>
	</test>


	<test name="OpenCart Test">
		<parameter name="url" value="https://www.facebook.com/" />
		<parameter name="browser" value="edge" />


		<classes>
			<class name="testngsesstions.FacebookTest"/>
		</classes>
	</test>


</suite>